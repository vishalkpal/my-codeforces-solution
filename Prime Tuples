#include <bits/stdc++.h>
using namespace std;

// time complexity error in this problem//


bool isprime(long n) 
{ 
    // Corner cases 
    if (n <= 1) 
        return false; 
    if (n <= 3) 
        return true; 
  
    // This is checked so that we can skip 
    // middle five numbers in below loop 
    if (n % 2 == 0 || n % 3 == 0) 
        return false; 
  
    for (int i = 5; i * i <= n; i = i + 6) 
        if (n % i == 0 || n % (i + 2) == 0) 
            return false; 
  
    return true; 
} 


int main() {
  // your code goes here
  int t;cin>>t;
  while(t--)
  {
     long n,c=0;
     //int a[3];
     cin>>n;

     for(long i=2;i<=n;i++)
     {
      bool x=0;
         x=isprime(i);
         
         if(x==1)
         {
          c++;
         // cout<<c;
        // i=i+i;
         }

     }
    // cout<<c<<endl;
     long long ncr =c*(c-1)*(c-2)/6;
     cout<<ncr<<endl;
  }
  return 0;
}
